	import pixel.ui.core.PixelUINS;

	use namespace PixelUINS;
	
	/**
	 * 复选框
	 **/
	public class UICheckButton extends UIControl
	{
		public function UICheckButton(style:Class = null)
		{
			super(style ? style:UICheckButtonStyle);
		}
	}
}